    <name>get_flights</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>AIMS</connection>
    <sql>SELECT T.*
FROM (
	SELECT
		aircraftregistration AS aircraftID
		, scheduleddeparture, scheduledArrival, actualdeparture, actualarrival
		, CAST(scheduleddeparture AS DATE) AS timeID
		, delaycode
		, CASE
			WHEN cancelled='Y' THEN 0
			ELSE 
				EXTRACT(HOUR FROM actualarrival - actualdeparture)
				+ EXTRACT(MINUTE FROM actualarrival - actualdeparture)/60
				+ EXTRACT(SECOND FROM actualarrival - actualdeparture)/3600
			END AS flightHours
		, CASE WHEN cancelled='Y' THEN 0 ELSE 1 END AS flightCycles
		, CASE WHEN cancelled='Y' THEN 1 ELSE 0 END AS cancellations
		, CASE WHEN delaycode IS NOT NULL THEN 1 ELSE 0 END AS delays
	FROM flights
	WHERE scheduledArrival > scheduleddeparture AND actualarrival > actualdeparture
) AS T
ORDER BY T.aircraftID, T.timeID, T.delaycode</sql>
    <limit>0</limit>
    <lookup/>
