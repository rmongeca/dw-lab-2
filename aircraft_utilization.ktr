  <step>
    <name>get_flights</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>AIMS</connection>
    <sql>SELECT
  aircraftregistration AS aircraftID
, flightId
, scheduleddeparture
, CAST(scheduleddeparture AS DATE) AS timeID
, actualarrival - actualdeparture AS flightHours
, CASE WHEN cancelled='Y' THEN 1 ELSE 0 END AS cancellations
, CASE WHEN delaycode IS NOT NULL THEN 1 ELSE 0 END AS delays
, delaycode
, EXTRACT(HOUR FROM
	CASE
		WHEN delaycode IS NOT NULL
			THEN actualdeparture - scheduleddeparture
		ELSE scheduleddeparture - scheduleddeparture
	END
	)*60
	+ EXTRACT(MINUTE FROM
	CASE
		WHEN delaycode IS NOT NULL
			THEN actualdeparture - scheduleddeparture
		ELSE scheduleddeparture - scheduleddeparture
	END
	) 
	+ EXTRACT(SECOND FROM
	CASE
		WHEN delaycode IS NOT NULL
			THEN actualdeparture - scheduleddeparture
		ELSE scheduleddeparture - scheduleddeparture
	END)/60
AS delayedMinutes
FROM flights
</sql>
