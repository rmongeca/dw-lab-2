  <step>
    <name>get_oper_interrupts</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>AMOS</connection>
    <sql>SELECT
	T.aircraftID, T.timeID, T.delaycode
	, CASE WHEN (delaycode IS NOT NULL AND NOT kind IN ('Revision','Safety'))
			THEN (
				EXTRACT(HOUR FROM duration)*60 
				+ EXTRACT(MINUTE FROM duration) 
				+ EXTRACT(SECOND FROM duration)/60
			)
		ELSE 0
		END AS delayedMinutes
	, CASE WHEN T.kind IN ('Maintenance','Revision') THEN 1 ELSE 0 END AS scheduled
	, CASE WHEN T.kind IN ('Delay', 'Safety', 'AircraftOnGround') THEN 1 ELSE 0 END AS unscheduled
FROM (
	SELECT
	  aircraftregistration AS aircraftID
	, CAST(departure AS DATE) AS timeID
	, kind
	, delaycode
	, SUM(duration) AS duration
	FROM operationinterruption
	WHERE delaycode IS NOT NULL
	GROUP BY aircraftID, timeID, kind, delaycode
) AS T
ORDER BY T.aircraftID, T.timeID, T.delaycode</sql>
